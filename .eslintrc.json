{
	"extends": [
		"next",
		"plugin:@typescript-eslint/recommended",
		"plugin:testing-library/react",
		"plugin:vitest/recommended",
		"prettier"
	],
	"plugins": ["@typescript-eslint"],
	"rules": {
		"no-console": ["warn", { "allow": ["warn", "error", "info"] }],
		"@typescript-eslint/no-explicit-any": "warn"
	}
}
