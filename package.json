{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"dev:test": "export NODE_ENV=test && next dev",
		"build": "next build",
		"build:test": "export NODE_ENV=\"test\" && next build",
		"start": "next start",
		"start:test": "export NODE_ENV=\"test\" && next start",
		"lint": "next lint",
		"test": "vitest --config ./vitest.config.ts --passWithNoTests",
		"test:run": "vitest run --config ./vitest.config.ts --passWithNoTests",
		"cypress:open": "env-cmd -f .env.test.local cypress open",
		"cypress:start": "start-server-and-test 'npm run start:test' 3000 'npm run cypress:open'",
		"cypress:build": "npm run build:test && npm run cypress:start",
		"cypress:dev": "start-server-and-test 'npm run dev:test' 3000 'npm run cypress:open'",
		"cypress:run": "start-server-and-test 'npm run start:test' 3000 'env-cmd -f .env.test.local cypress run'",
		"prepare": "husky install"
	},
	"dependencies": {
		"@ant-design/icons": "^5.3.1",
		"@ant-design/nextjs-registry": "^1.0.0",
		"@tanstack/react-query": "^5.22.2",
		"@tanstack/react-query-devtools": "^5.23.0",
		"antd": "^5.14.1",
		"axios": "^1.6.7",
		"clsx": "^2.1.0",
		"compressorjs": "^1.2.1",
		"next": "14.1.0",
		"next-auth": "^5.0.0-beta.13",
		"react": "^18",
		"react-daum-postcode": "^3.1.3",
		"react-dom": "^18",
		"recoil": "^0.7.7"
	},
	"devDependencies": {
		"@testing-library/cypress": "^10.0.1",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^14.2.1",
		"@testing-library/user-event": "^14.5.2",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.0.1",
		"cypress": "^13.6.6",
		"env-cmd": "^10.1.0",
		"eslint": "^8",
		"eslint-config-next": "14.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-cypress": "^2.15.1",
		"eslint-plugin-jest-dom": "^5.1.0",
		"eslint-plugin-testing-library": "^6.2.0",
		"eslint-plugin-vitest": "^0.3.22",
		"husky": "^9.0.11",
		"jsdom": "^24.0.0",
		"lint-staged": "^15.2.2",
		"msw": "^2.2.1",
		"postcss": "^8",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"start-server-and-test": "^2.0.3",
		"tailwindcss": "^3.3.0",
		"typescript": "^5",
		"vitest": "^1.2.2"
	}
}
